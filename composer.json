{
  "name": "am2tec/cron",
  "description": "Controle da cron em php e postgres",
  "keywords": [
    "cron",
    "crontab",
    "tasks manager",
    "schedule"
  ],
  "homepage": "https://cron.multimate.ru",
  "license": "MIT",
  "authors": [
    {
      "name": "AM2tec",
      "email": "am2tec@suport.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "mtdowling/cron-expression": "1.2.*"
  },
  "suggest": {
    "monolog/monolog": "Allow writing log messages"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "codeclimate/php-test-reporter": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "am2tec\\crontab\\": "src/",
      "am2tec\\crontab_tests\\": "tests/"
    }
  }
}
